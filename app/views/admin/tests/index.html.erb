<h1><%= t('.header') %></h1>
<%= button_to t('.new_test'), new_admin_test_path, class: "btn btn-success mb-2", method: :get %>
<table class="table table-striped table-bordered">
  <thead class="thead-dark" align="center">
    <th><%= t('.title') %></th>
    <th><%= t('.level') %></th>
    <th><%= t('.questions') %></th>
    <th colspan="3"><%= t('.actions') %></th>
  </thead>

  <tbody>
    <%= render @tests %>
  </tbody>
</table>
