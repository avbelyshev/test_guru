<tr>
  <td><%= test.title %></td>
  <td><%= test_level(test) %></td>
  <td align="center"><%= test.questions.count %></td>
  <td align="center"><%= button_to t('.show'), admin_test_path(test), class: "btn btn-primary btn-sm", method: :get %></td>
  <td align="center"><%= button_to t('.edit'), edit_admin_test_path(test), class: "btn btn-warning btn-sm", method: :get %></td>
  <td ><%= button_to t('.delete'),
                  admin_test_path(test),
                  method: :delete,
                  data: { confirm: t('.delete_confirm') },
                  class: "btn btn-danger btn-sm" %>
  </td>
</tr>
